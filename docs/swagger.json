            "get": {
                "tags": [
                    "Menu"
                ],
                "summary": "Get min price of each food type",
                "operationId": "GetMenuMinPrice",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "number"
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request"
                    },
                    "500": {
                        "description": "Internal Server Error"
